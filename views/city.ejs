<div style="margin:20px">
    <div class="section">
        <div id="imageCity"></div>
      <!--  <img src="../resources/paris.jpg" alt="Paris" style="width:200px;height:auto;border-radius: 50%;">-->
        <div class="text">
            <h3 style=" font-size: 2em; font-weight: bold;" id="place"></h3>

            <div id="loadProgressbar" class="progress" style="width: 200px;height:5px"> </div>

            <p id="loadProgress"> </p>

            <a class="btn btn-dark btn-sm" id="buttonRoute">See Route</a>
        </div>
        <script>
          

            // Adapted from examples on the Querystring homepage.
            const queryString = window.location.search;
            const urlParams = new URLSearchParams(queryString);
            const place = urlParams.get('place') 
            // console.log("PLACE IS " + place);
            document.getElementById('place').innerHTML = place;

            var a = document.getElementById('buttonRoute'); //or grab it by tagname etc
            a.href = "/map?city=" + place;

            var sectionContainer = document.getElementById("imageCity");
            var image = document.createElement("img");
            image.src = "../resources/"+place+".jpg";
            image.setAttribute(
                'style',
                'width:200px;height:auto;border-radius: 50%;',
            );
            sectionContainer.appendChild(image);
          
        </script>
    </div>

    <div style="margin-left: 40px;">Attractions:</div>

    <div id="loadAttractions"> </div>
    <div style="width:300px">
        <canvas id="myChart"></canvas>
    </div>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.1/jquery.min.js"></script>
    <script src="./js/get-attractions.js"></script>
    <script src="http://cdn.jsdelivr.net/npm/chart.js"></script>

    <script>
        const ctx = document.getElementById('myChart');
        ctx.width = 100;
        new Chart(ctx, {
            type: 'line',
            data: {
                labels: ['1', '2', '3', '4', '5', '6'],
                datasets: [{
                    label: 'Elevation',
                    data: [12, 19, 30, 50, 20, 30], //Height Data
                    borderWidth: 1
                }]
            },
            options: {
                scales: {
                    y: {
                        beginAtZero: true
                    }
                }
            }
        });
